<table class="sketch" summary="Your sketch">
	<tr>
		<td>
			<h1> Your Sketch </h1>
			<h3> About:  <%= @sketch.about %></h3>
			<p> Gender: <%= @sketch.gender %> </p>
			<h4> Mobile: <%= @sketch.mobile %> </h4>
			<h4> Interest <%= @sketch.interest %> </h4>
			<h4> Occupation <%= @sketch.occupation %> </h4>
			<p> <%= link_to "Repaint your sketch", sketches_edit_path %> </p>
			<p> <%= link_to "MyUrl", @sketch %> </p>
		</td>
	</tr>
	<tr>
		<td>
			<%= render 'shared/stats' %>
			<%= link_to "See friends", user_friends_path(@user) %> 
			<%= link_to "See subscriptions", user_subscribers_path(@user) %> 
		</td>
	</tr>
</table>